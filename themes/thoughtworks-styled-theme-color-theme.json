{
	"name": "Thoughtworks Style Theme",
	"colors": {
    // 编辑器背景与文本
    "editor.background": "#003d4f",
    "editor.foreground": "#edf1f3",
    "editor.lineHighlightBackground": "#003d4f",
    "editor.selectionBackground": "#634f7d",
    "editor.selectionHighlightBackground": "#634f7d",
    "editorCursor.foreground": "#f2617a",
    "editor.lineHighlightBorder": "#6b9e78",
    "editorWhitespace.foreground": "#666666",
    "editorLineNumber.foreground": "#666666",
    "editorLineNumber.activeForeground": "#f2617a",
    "editorRuler.foreground": "#003d4f",
    "editorBracketMatch.background": "#634f7d",
    "editorBracketMatch.border": "#edf1f3",
    
    // 活动栏与标题栏
    "activityBar.background": "#003d4f",
    "activityBar.foreground": "#edf1f3",
    "activityBarBadge.background": "#f2617a",
    "activityBarBadge.foreground": "#ffffff",
    "titleBar.activeBackground": "#003d4f",
    "titleBar.activeForeground": "#f2617a",
    "titleBar.inactiveBackground": "#003d4f",
    "titleBar.inactiveForeground": "#edf1f3",
    
    // 侧边栏
    "sideBar.background": "#003d4f",
    "sideBar.foreground": "#edf1f3",
    "sideBarSectionHeader.background": "#003d4f",
    "sideBarSectionHeader.foreground": "#edf1f3",
    "sideBarTitle.foreground": "#edf1f3",
    
    // 状态栏
    "statusBar.background": "#f2617a",
    "statusBar.foreground": "#edf1f3",
    "statusBar.noFolderBackground": "#666666",
    "statusBar.noFolderForeground": "#edf1f3",
    "statusBar.debuggingBackground": "#cc850a",
    "statusBar.debuggingForeground": "#000000",
    
    // 终端
    "terminal.background": "#003d4f",
    "terminal.foreground": "#edf1f3",
    "terminal.ansiBlack": "#000000",
    "terminal.ansiRed": "#f2617a",
    "terminal.ansiGreen": "#6b9e78",
    "terminal.ansiYellow": "#cc850a",
    "terminal.ansiBlue": "#47a1ad",
    "terminal.ansiMagenta": "#634f7d",
    "terminal.ansiCyan": "#003d4f",
    "terminal.ansiWhite": "#edf1f3",
    "terminalCursor.foreground": "#f2617a",
    
    // 输入框与列表
    "input.background": "#47a1ad",
    "input.foreground": "#edf1f3",
    "input.placeholderForeground": "#666666",
    "input.border": "#47a1ad",
    "list.activeSelectionBackground": "#6b9e78",
    "list.activeSelectionForeground": "#edf1f3",
    "list.inactiveSelectionBackground": "#003d4f",
    "list.inactiveSelectionForeground": "#edf1f3",
    "list.hoverBackground": "#47a1ad",
    "list.hoverForeground": "#edf1f3",
    
    // 面板（控制台、输出窗口等）
    "panel.background": "#003d4f",
    "panel.border": "#6b9e78",
    "panelTitle.activeForeground": "#edf1f3",
    "panelTitle.inactiveForeground": "#666666",
    
    // 按钮
    "button.background": "#f2617a",
    "button.foreground": "#ffffff",
    "button.hoverBackground": "#cc850a",
    
    // 滚动条
    "scrollbarSlider.background": "#666666",
    "scrollbarSlider.hoverBackground": "#6b9e78",
    "scrollbarSlider.activeBackground": "#47a1ad",
    
    // 分隔线
    "activityBar.border": "#47a1ad",
    "sideBar.border": "#47a1ad",
    "editorGroup.border": "#47a1ad",
		// tab
		"tab.activeBackground": "#f2617a",
		"tab.inactiveBackground": "#003d4f",
		"tab.border": "#6b9e78",
		"editorGroupHeader.tabsBackground": "#003d4f",
		"editorGroupHeader.border": "#003d4f"

  },
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#666666"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#edf1f3"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#47a1ad"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#f2617a"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
				],
			"settings": {
				"foreground": "#cc850a"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.control",
				"constant.other.color",
				// "punctuation",
				// "meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#f2617a"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#f2617a"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#cc850a"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other"
			],
			"settings": {
				"foreground": "#6b9e78"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#cc850a"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#f2617a"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"foreground": "#C3E88D"
			}
		},
		{
			"name": "Class, Support",
			"scope": [
				"entity.name",
				"support.type",
				"support.class",
				"support.other.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#B2CCD6"
			}
		},
		{
			"name": "Entity Types",
			"scope": [
				"support.type"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "CSS Class and Support",
			"scope": [
				"source.css support.type.property-name",
				"source.sass support.type.property-name",
				"source.scss support.type.property-name",
				"source.less support.type.property-name",
				"source.stylus support.type.property-name",
				"source.postcss support.type.property-name"
			],
			"settings": {
				"foreground": "#C3E88D"
			}
		},
		{
			"name": "Sub-methods",
			"scope": [
				"entity.name.module.js",
				"variable.import.parameter.js",
				"variable.other.class.js"
			],
			"settings": {
				"foreground": "#f2617a"
			}
		},
		{
			"name": "Language methods",
			"scope": [
				"variable.language"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#634f7d"
			}
		},
		{
			"name": "entity.name.method.js",
			"scope": [
				"entity.name.method.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#6b9e78"
			}
		},
		{
			"name": "meta.method.js",
			"scope": [
				"meta.class-method.js entity.name.function.js",
				"variable.function.constructor"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Attributes",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#cc850a"
			}
		},
		{
			"name": "HTML Attributes",
			"scope": [
				"text.html.basic entity.other.attribute-name.html",
				"text.html.basic entity.other.attribute-name"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#f2617a"
			}
		},
		{
			"name": "CSS Classes",
			"scope": [
				"entity.other.attribute-name.class"
			],
			"settings": {
				"foreground": "#B2CCD6"
			}
		},
		{
			"name": "CSS ID's",
			"scope": [
				"source.sass keyword.control"
			],
			"settings": {
				"foreground": "#634f7d"
			}
		},
		{
			"name": "Inserted",
			"scope": [
				"markup.inserted"
			],
			"settings": {
				"foreground": "#C3E88D"
			}
		},
		{
			"name": "Deleted",
			"scope": [
				"markup.deleted"
			],
			"settings": {
				"foreground": "#f2617a"
			}
		},
		{
			"name": "Changed",
			"scope": [
				"markup.changed"
			],
			"settings": {
				"foreground": "#cc850a"
			}
		},
		{
			"name": "Regular Expressions",
			"scope": [
				"string.regexp"
			],
			"settings": {
				"foreground": "#89DDFF"
			}
		},
		{
			"name": "Escape Characters",
			"scope": [
				"constant.character.escape"
			],
			"settings": {
				"foreground": "#89DDFF"
			}
		},
		{
			"name": "URL",
			"scope": [
				"*url*",
				"*link*",
				"*uri*"
			],
			"settings": {
				"fontStyle": "underline",
				"foreground": "#f2617a"
			}
		},
		{
			"name": "Decorators",
			"scope": [
				"tag.decorator.js entity.name.tag.js",
				"tag.decorator.js punctuation.definition.tag.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#82AAFF"
			}
		},
		{
			"name": "ES7 Bind Operator",
			"scope": [
				"source.js constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#FF5370"
			}
		},
